#shangpin{
    background-color #fff
    position absolute !important
    left 0
    top 0
    z-index 60;
}
header {
  margin: 10px;
  position: relative;

  ul {
    display: flex;
    justify-content: space-around;

    li {
      margin-left: 19px;
      font-size: rem(19px);
    }
  }
}

.header-img img {
  width: 18px;
  position: absolute;
  top: 0;
  left: 25px;
}

.goback p {
  line-height: 25px;
  font-size: 22px;
  position: absolute;
  top: 0;
  left: 0;
}

.main-img img {
  width: 100%;
}

.main-money {
  margin: 10px 10px;
}

.main-money span {
  font-size: 22px;
  font-weight: bold;
  color: #DC0C0C;
}

.mian-txt {
  margin: 10px 10px;
  display: flex;
  justify-content: space-between;
}

.mian-txt span {
  font-weight: bold;
  font-size: 14px;
}

.mian-txt p {
  font-size: 10px;
  color: #D62F81;
}

.mian-txt img {
  width: 10px;
}

.changtiao {
  height: 20px;
}

.changtiao img {
  width: 100%;
}

.guige {
  margin: 10px;
  display: flex;
}

.guige p {
  margin-right: 10px;
}

.canshu {
  overflow: hidden;
  width: 100%;
  height: rem(400px);
  z-index: 3;
  position: absolute;
  bottom: 0;
  background: #fff;
}

#shangpin {
  position: relative;
}

.canshu-header {
  margin: 10px 150px;
}

.canshu ul {
  width: 100%;

  li {
    padding: 6px 18px;
    border-bottom: solid 1px;
  }
}

.canshu button {
  width: 100%;
  height: rem(30px);
  margin: 5px 0;
  background: #DC0C0C;
  border: solid 1px #DC0C0C;
}

.guigeyuyue {
  overflow: hidden;
  width: 100%;
  height: rem(400px);
  z-index: 3;
  position: absolute;
  bottom: 0;
  background: #fff;
}

.gb {
  position: absolute;
  top: 5px;
  right: 5px;
}

.chacha {
  width: 20px;
}

.guigeyuyue {
  .guigeyuyue-header {
    display: flex;

    .g-h-i img {
      width: 150px;
    }

    .g-h-t {
      font-size: 16px;
      margin: 0 5px;
      line-height: 150px;
      color: #DC0C0C;
    }
  }
}

.guigeyuyue-fenlei {
  margin: 20px 0;
  width: 100%;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.g-f-txtt {
  font-size 15px
  color: #666666;
  margin: 3px;
  width: 28%;
  background: #F5F5F5;
  text-align: center;
  padding: 8px 4px;
}

.g-f-img {
  width: 100%;
  height: 80px;
}

.guigeyuyue-num {
  display: flex;
}

.guigeyuyue-foot {
  width: 100%;
  height: 40px;
  border: solid 1px;
  line-height: 40px;
  text-align: center;
  color: white;
  background: #C70E60;
}

.pingjia {
  .pingjia-head {
    margin: 8px;
    height: 30px;

    span {
      line-height: 30px;
      font-weight: bold;
      float: right;
    }

    p {
      font-weight: bold;
      line-height: 30px;
      float: left;
    }
  }
}

.p-m-t span {
  font-size: 15px;
  margin: 0 8px;
}

.s-h-i-img {
  width: 40px;
}

.shangpu-head {
  display: flex;
  justify-content: space-around;
  margin-top: 10px;
}

.shangpu-h-all span {
  
  border: dotted 1px;
  padding: 2px 4px;
  margin-left: 2px;
}

.shangpu-h-go span {
  margin-left: 2px;
 padding: 2px 4px;
  background: #FF3366;
  border: dotted 1px white;
  color: white;
}

.shangpu-foot {
  height: 40px;
  width: 100%;
}

.shangpu-foot ul {
  justify-content: space-around;
  display: flex;
}

.shangpu-foot li {
  line-height: 40px;
  font-size: 10px;
}

.shangpu-foot li p {
  font-size: 8px;
  float: right;
  color: #D61515;
}

.shangpu-h-txt span {
  font-size: 14px;
}

.shangpu-h-txt p {
  color: #999999;
  font-size: 13px;
}